{
  "name": "can-bundle-it",
  "version": "3.0.2",
  "description": "A command line tool that detect JavaScript file can be bundled.",
  "keywords": [
    "browser",
    "bundle",
    "cli",
    "detect",
    "tool",
    "webpack"
  ],
  "homepage": "https://github.com/azu/can-bundle-it",
  "bugs": {
    "url": "https://github.com/azu/can-bundle-it/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/can-bundle-it.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/can-bundle-it.mjs",
  "types": "lib/can-bundle-it.d.ts",
  "bin": {
    "can-bundle-it": "bin/cmd.mjs"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.mts\"",
    "watch": "tsc -p . --watch"
  },
  "dependencies": {
    "meow": "^10.1.5",
    "node-libs-browser": "^2.2.1",
    "tempfile": "^4.0.0",
    "webpack": "^5.90.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.15",
    "cross-env": "^7.0.3",
    "mocha": "^10.8.1",
    "ts-node": "^10.9.2",
    "ts-node-test-register": "^10.0.0",
    "typescript": "^4.7.4"
  }
}
